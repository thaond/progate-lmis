<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_2_0.dtd">

<service-builder package-path="larion.progate.cds">
	<namespace>ProGateManagement</namespace>

	<!-- Start CDS -->

	<entity name="Competency" table="cds_competencies"
		local-service="true" remote-service="true" cache-enabled="false">
		<!-- PK fields -->
		<column name="competencyId" db-name="id" type="Integer"
			primary="true" id-type="sequence" id-param="cds_competencies_id_seq" />
		<column name="rootId" db-name="root_id" type="int"></column>
		<column name="name" db-name="name" type="String"></column>
		<column name="description" db-name="description" type="String"></column>
		<column name="numberOrder" db-name="number_order" type="int"></column>
		<column name="createdBy" db-name="created_by" type="int"></column>
		<column name="createdAt" db-name="created_at" type="Date"></column>
		<column name="updatedBy" db-name="updated_by" type="int"></column>
		<column name="updatedAt" db-name="updated_at" type="Date"></column>
		<column name="level" type="Collection" entity="Level"
			mapping-key="competencyId" />
		<column name="cdsTitleCompetencies" type="Collection" entity="CdsTitleCompetencies"
			mapping-key="competencyId" />

		<order>
			<order-column name="numberOrder" order-by="asc"></order-column>
		</order>

		<finder return-type="Collection" name="name">
			<finder-column name="name" case-sensitive="true"
				comparator="LIKE"></finder-column>
		</finder>

		<finder return-type="Collection" name="rootId">
			<finder-column name="rootId" comparator="="></finder-column>
		</finder>

		<finder name="numberOrderGreater" return-type="Collection">
			<finder-column name="numberOrder" comparator="&gt;"></finder-column>
		</finder>
	</entity>

	<entity name="Level" table="cds_levels" local-service="true"
		remote-service="true" cache-enabled="false">
		<!-- PK fields -->
		<column name="rootId" db-name="root_id" type="int"></column>
		<column name="levelId" db-name="id" type="Integer" primary="true"
			id-type="sequence" id-param="cds_levels_id_seq" />
