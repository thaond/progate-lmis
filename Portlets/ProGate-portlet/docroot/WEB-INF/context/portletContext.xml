	</bean>
	
	<bean id="evaluationPeriodsController"
	class="com.larion.progate.cds.configuration.controllers.EvaluationPeriodsController">
		<property name="viewName" value="cds/configuration/evaluationPeriods/evaluationPeriodsScreen"></property>
	</bean>

	<bean id="configTitleController"
	class="com.larion.progate.cds.configuration.controllers.ConfigTitleController">
		<property name="viewName" value="cds/configuration/configTitle/configTitleScreen"></property>
	</bean>
	
	<bean id="configMinimumReqController"
	class="com.larion.progate.cds.configuration.controllers.ConfigMinimumReqController">
		<property name="viewName" value="cds/configuration/configMinimumReq/configMinimumReqScreen"></property>
	</bean>
	
	<bean id="slotInformationController"
	class="com.larion.progate.cds.evaluation.controllers.SlotInformationController">
		<property name="viewName" value="cds/evaluation/slotInformationScreen"></property>
	</bean>
