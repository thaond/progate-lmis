<!-- BEGIN LMIS -->

	<bean id="changeWorkingTimeController"
		class="com.larion.progate.lmis.controllers.ChangeWorkingTimeController">
		<property name="viewName" value="lmis/attendanceInfo/changeWorkingTime/changeWorkingTime"></property>
	</bean>
